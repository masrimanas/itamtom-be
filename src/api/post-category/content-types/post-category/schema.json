      "unique": true,
      "required": true
    },
    "categoryUid": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true
    },
    "categoryName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "minLength": 10,
      "unique": true
    },
    "post": {
      "type": "relation",
